{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
     {
       "name": "client",
       "image": "prince220888/multi-client",
       "hostname": "client",
       "essential": true,
       "memory": 128
     },
     {
       "name": "server",
       "image": "prince220888/multi-server",
       "hostname": "api",
       "essential": false,
       "memory": 128
     },
     {
       "name": "worker",
       "image": "prince220888/multi-worker",
       "hostname": "worker",
       "essential": false,
       "memory": 128
     },
     {
       "name": "nginx",
       "image": "prince220888/multi-nginx",
       "hostname": "nginx",
       "essential": true,
       "portMappings": [
         {
           "hostPort": 80,
           "containerPort": 80
         }
       ],
       "links": ["client", "server"],
       "memory": 128
     }
  ]
}
